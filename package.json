{
  "name": "@ajycc20/react-pullload",
  "version": "0.0.2-alpha.0",
  "description": "pull refresh & load, based on better-scroll",
  "main": "lib/index.umd.js",
  "types": "lib/index.d.ts",
  "module": "lib/index.esm.js",
  "scripts": {
    "build": "rimraf ./lib && rollup -c",
    "lint": "eslint src --fix --ext .js,.jsx,.ts,.tsx",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajycc20/react-pullload.git"
  },
  "keywords": [
    "pull-refresh",
    "pull-load",
    "better-scroll",
    "react"
  ],
  "author": "ajycc20 <ajycc20@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ajycc20/react-pullload/issues"
  },
  "homepage": "https://github.com/ajycc20/react-pullload#readme",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@release-it/conventional-changelog": "^3.3.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@types/react": "^17.0.37",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.5.0",
    "eslint-config-alloy": "^4.4.0",
    "eslint-plugin-react": "^7.27.1",
    "husky": "4.3.8",
    "lint-staged": "^12.1.3",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "release-it": "^14.11.8",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "sass": "^1.45.1",
    "tslib": "^2.3.1",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.5",
    "@babel/runtime-corejs3": "^7.16.5",
    "@better-scroll/core": "^2.4.2",
    "@better-scroll/observe-dom": "^2.4.2",
    "@better-scroll/observe-image": "^2.4.2",
    "@better-scroll/pull-down": "^2.4.2",
    "@better-scroll/pull-up": "^2.4.2",
    "react": "^17.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --parser=typescript --write"
    ],
    "*.{html,css,scss}": [
      "prettier --write"
    ]
  },
  "directories": {
    "lib": "lib"
  }
}
